        <name>Populate course modules onchange</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (isLoading || newValue === '') {
        return;
    }
    var tech = g_form.getReference('u_bat', setModules);

    function setModules(batch) {

        if (batch.u_bat_tech == 'ServiceNow') {
            g_form.addInfoMessage('Course Modules for ServiceNow have been populated.');
            g_form.clearOptions('u_course_mod');
            g_form.addOption('u_course_mod', '0', 'Not Started');
            g_form.addOption('u_course_mod', '1', '1. ServiceNow Fundamentals');
            g_form.addOption('u_course_mod', '2', '2. Automation');
            g_form.addOption('u_course_mod', '3', '3. Client-side Scripting');
            g_form.addOption('u_course_mod', '4', '4. Server-side Scripting');
            g_form.addOption('u_course_mod', '5', '5. ServiceNow Application Development');
            g_form.addOption('u_course_mod', '6', '6. Performance Analytics');
            g_form.addOption('u_course_mod', '7', '7. Service Portal');
            g_form.addOption('u_course_mod', '8', '8. Individual Project');
            g_form.addOption('u_course_mod', '9', '9. Team Project');
            g_form.addOption('u_course_mod', '10', '10. Group Project');
        }

        if (batch.u_bat_tech == 'Pega System') {
            g_form.addInfoMessage('Course Modules for Pega System have been populated.');
            g_form.clearOptions('u_course_mod');
            g_form.addOption('u_course_mod', '0', 'Not Started');
            g_form.addOption('u_course_mod', '1', '1. Introduction to Pega');
            g_form.addOption('u_course_mod', '2', '2. Case Management');
            g_form.addOption('u_course_mod', '3', '3. Data Modeling');
            g_form.addOption('u_course_mod', '4', '4. Validation');
            g_form.addOption('u_course_mod', '5', '5. Information Exchange');
            g_form.addOption('u_course_mod', '6', '6. User Interface');
            g_form.addOption('u_course_mod', '7', '7. Application Development');
            g_form.addOption('u_course_mod', '8', '8. Individual Project');
            g_form.addOption('u_course_mod', '9', '9. Team Project');
            g_form.addOption('u_course_mod', '10', '10. Group Project');
        }
    }

    //Type appropriate comment here, and begin script below

}]]></script>
